package egovframework.example.sample.service;

public class ShimtuVO {
	private int OBJT_ID;		//프라이머리키
	private String SHLTR_NM;	//쉼터 명
	private String ROAD_NM;		//고속도로 명
	private String ROAD_DRC;	//회덕기점 + 논산 종점 //고속도로 방향?
	private String TOILET_YN;	//화장실 유무
	private String MING_NM;		//관리 본부
	private String MING_TEL;	//관리 본부 전화번호
	private String ADRES;		//쉼터 주소
	private String RN_ADRES;	//도로명 주소
	private long x;				// X좌표
	private long y;				// Y좌표
	
	
	public int getOBJT_ID() {
		return OBJT_ID;
	}
	public void setOBJT_ID(int oBJT_ID) {
		OBJT_ID = oBJT_ID;
	}
	public String getSHLTR_NM() {
		return SHLTR_NM;
	}
	public void setSHLTR_NM(String sHLTR_NM) {
		SHLTR_NM = sHLTR_NM;
	}
	public String getROAD_NM() {
		return ROAD_NM;
	}
	public void setROAD_NM(String rOAD_NM) {
		ROAD_NM = rOAD_NM;
	}
	public String getROAD_DRC() {
		return ROAD_DRC;
	}
	public void setROAD_DRC(String rOAD_DRC) {
		ROAD_DRC = rOAD_DRC;
	}
	public String getTOILET_YN() {
		return TOILET_YN;
	}
	public void setTOILET_YN(String tOILET_YN) {
		TOILET_YN = tOILET_YN;
	}
	public String getMING_NM() {
		return MING_NM;
	}
	public void setMING_NM(String mING_NM) {
		MING_NM = mING_NM;
	}
	public String getMING_TEL() {
		return MING_TEL;
	}
	public void setMING_TEL(String mING_TEL) {
		MING_TEL = mING_TEL;
	}
	public String getADRES() {
		return ADRES;
	}
	public void setADRES(String aDRES) {
		ADRES = aDRES;
	}
	public String getRN_ADRES() {
		return RN_ADRES;
	}
	public void setRN_ADRES(String rN_ADRES) {
		RN_ADRES = rN_ADRES;
	}
	public long getX() {
		return x;
	}
	public void setX(long x) {
		this.x = x;
	}
	public long getY() {
		return y;
	}
	public void setY(long y) {
		this.y = y;
	}
	
	
}
